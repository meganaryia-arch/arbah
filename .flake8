[flake8]
max-line-length = 88
extend-ignore = E203, W503, E501
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    *.egg-info,
    .mypy_cache,
    .pytest_cache,
    .tox,
    docs
per-file-ignores =
    __init__.py:F401
    tests/*:F401,F811
max-complexity = 10
import-order-style = google
application-import-names = arbah